{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/event-order-app/back-end/src/index.ts",  // Path ke file entry point
      "runtimeExecutable": "npx",  // Menjalankan ts-node melalui npx
      "runtimeArgs": [
        "ts-node"  // Menjalankan ts-node
      ],      "outFiles": [
        "${workspaceFolder}/dist/**/*.js"  // Output JavaScript setelah di-compile
      ]
    }
  ]
}